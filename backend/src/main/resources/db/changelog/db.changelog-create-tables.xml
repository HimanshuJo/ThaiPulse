    <changeSet id="create-bangkok-news" author="himanshh">
        <createTable tableName="bangkok_news">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="title" type="VARCHAR(500)">
                <constraints nullable="false"/>
            </column>
            <column name="source" type="VARCHAR(100000)"/>
            <column name="link" type="VARCHAR(500)">
                <constraints unique="true"/>
            </column>
            <column name="image" type="VARCHAR(5000)"/>
            <column name="published_date" type="VARCHAR(500)"/>
        </createTable>
